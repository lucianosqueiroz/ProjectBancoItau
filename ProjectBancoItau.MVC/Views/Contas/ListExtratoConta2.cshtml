@model IEnumerable<ProjectBancoItau.MVC.ViewModels.ClienteContaTransLogTransViewModel>

@{
    ViewBag.Title = "ListExtratoConta2";
}


<div class="container" style="color:blue">
    <p>
        <h4 style="margin-top:5%">@Html.ViewBag.nomeCliente</h4>
        <h5 style="margin-left:5%">@Html.ViewBag.dadosConta</h5>
    </p>
    <table class="table">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Transacao)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DataTrans)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ValorTrans)
            </th>
            <th></th>
        </tr>

        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Transacao)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.DataTrans)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.ValorTrans)
                </td>
                
            </tr>
        }

    </table>
    @Html.ActionLink("Imprimir", "Edit", new { /* id=item.PrimaryKey */ })
</div>
